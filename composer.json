{
    "name": "conversion/eventcalendar",
    "type": "typo3-cms-extension",
    "description": "Event calendar with VueJS output",
    "authors": [
        {
            "name": "Mikel Wohlschlegel",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5",
        "helhum/dotenv-connector": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Conversion\\Eventcalendar\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conversion\\Eventcalendar\\Tests\\": "Tests"
        }
    },
    "replace": {
        "eventcalendar": "self.version",
        "typo3-ter/eventcalendar": "self.version"
    },
    "extra": {
        "helhum/dotenv-connector": {
            "env-file": ".env"
        }
    }
}